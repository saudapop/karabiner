{
  "title": "FC660C Arrow Keys",
  "rules": [
    {
      "description": "FC660C Arrow Keys (left_command + ijkl to arrow keys)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": ["left_gui"],
              "optional": ["left_shift", "fn", "left_alt"]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["left_gui"],
              "optional": ["left_shift", "fn", "left_alt"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["left_gui"],
              "optional": ["left_shift", "fn", "left_alt"]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["left_gui"],
              "optional": ["left_shift", "fn", "left_alt"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ]
        }
      ]
    }
  ]
}
